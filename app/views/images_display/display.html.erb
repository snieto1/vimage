<body class="display_body">

<div class="logo23"><%=image_tag("logo4.png", options={})%></div>

<div class="form"> <%= form_tag(images_display_path, method: "get") do %>
  <%= label_tag(:q, "Just VIMAGE it") %>
  <font color="black" class="search_bar"><%= text_field_tag(:q) %></font>
  <font color="black"><%= submit_tag("GO") %></font>
  <% end %>
</div>

<h5 class="title">videos & images on one page </h5>

<hr></hr>

<div class="video">
<% @render2.each do |video| %>
  <iframe class:"video_style" width="560" height="315" src="https://www.youtube.com/embed/<%=video%>" frameborder="0" allowfullscreen></iframe>
<% end %>
</div>

<div class="images">
<% @render.each do |image| %>
  <img class="img-responsive" alt="Responsive image" src="<%=image%>" />
<% end %>
</div>



<div class="images">
<% @render3.each do |image| %>
  <img class="img-responsive" alt="Responsive image" src="<%=image%>" />
<% end %>
</div>

</body>
